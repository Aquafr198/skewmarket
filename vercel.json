{
  "rewrites": [
    {
      "source": "/api/polymarket/:path*",
      "destination": "https://gamma-api.polymarket.com/:path*"
    },
    {
      "source": "/api/news/:path*",
      "destination": "https://news.google.com/rss/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
